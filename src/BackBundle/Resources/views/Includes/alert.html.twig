{% for type, messages in app.session.flashBag.all %}    <div class="row">        <div class="col-md-12 col-sm-12">            {% for message in messages %}                <div class="{{ type }}">                    <button type="button" class="close" data-dismiss="alert"                            aria-hidden="true"></button>                    {{ message | raw }}                </div>            {% endfor %}        </div>    </div>{% endfor %}