{% extends "BackBundle::layout.html.twig" %}{#Include CSS#}  {% block stylesheets %}      {{ parent() }}      {% stylesheets      'bundles/back/theme/assets/global/plugins/select2/css/select2.min.css'      'bundles/back/theme/assets/global/plugins/select2/css/select2-bootstrap.min.css'      'bundles/back/theme/assets/global/plugins/bootstrap-fileinput/bootstrap-fileinput.css'      'bundles/back/css/tables.css'      'bundles/back/css/style.css'      filter='cssrewrite' %}      <link rel="stylesheet" href="{{ asset_url }}"/>      {% endstylesheets %}      <style type="text/css">      </style>  {% endblock stylesheets %}    {% block title %}    Ajout société | {{ parent() }}{% endblock %}    {% block header_breadcrumb %}        <li>            <span>Ajout</span>        </li>    {% endblock header_breadcrumb %}{% block action_toolbar %}    <div class="btn-group pull-right">        <button type="button" class="btn green btn-sm btn-outline dropdown-toggle"                data-toggle="dropdown"> Actions            <i class="fa fa-angle-down"></i>        </button>        <ul class="dropdown-menu pull-right" role="menu">            <li>                <a href="{{ path('back_company_list') }}">                    <i class="icon-list"></i> Liste de sociétés</a>            </li>            {#<li>#}            {#<a href="{{ path('back_company_add') }}">#}            {#<i class="icon-shield"></i> Ajout société</a>#}            {#</li>#}            <li class="divider"></li>            <li>                <a href="#">                    <i class="icon-bag"></i> Autres</a>            </li>        </ul>    </div>{% endblock action_toolbar %}    {#{% block page_title %}Clients{% endblock page_title %}#}    {#{% block page_sub_title %}#}        {#<small>Liste</small>{% endblock page_sub_title %}#}{% block back_body %}    <div class="row" id="">        <div class="col-md-12 col-sm-12 margin-top-20">            <!-- BEGIN Portlet PORTLET-->            <div class="portlet">                <div class="portlet-title">                    <div class="caption font-green-sharp">                        <i class="icon-briefcase font-green-sharp"></i>                        <span class="caption-subject bold uppercase"> Société</span>                        <span class="caption-helper"> Ajout</span>                    </div>                    <div class="actions">                        <a href="{{ path('back_company_list') }}" title="Ajouter un company"                           class="btn btn-sm btn-block green btn-outline"><span class="fa fa-lsit"></span> Liste de                            Sociétés</a>                    </div>                </div>                <div class="portlet-body">                    {% include('BackBundle:Includes:alert.html.twig') %}                    {{ form_start(form, {'attr': {'id': 'add_app_form','autocomplete':'off'}}) }}                    <div class="row">                        <div class="col-lg-8 col-md-8">                            <div class="row">                                {% if form.name is defined %}                                    <div class="col-sm-4">                                        <div class="form-group">                                            {{ form_label(form.name) }}                                            {#<div class="input-group">                                                <span class="input-group-addon">                                                <i class="fa fa-building-o"></i>                                                </span>#}                                                {{ form_widget(form.name) }}                                            {#</div>#}                                        </div>                                    </div>                                {% endif %}                                {% if form.siret is defined %}                                    <div class="col-sm-4">                                        <div class="form-group">                                            {{ form_label(form.siret) }}                                            {#<div class="input-group">#}                                                            {#<span class="input-group-addon">#}                                                            {#<i class="fa fa-key"></i>#}                                                            {#</span>#}                                                {{ form_widget(form.siret) }}                                            {#</div>#}                                        </div>                                    </div>                                {% endif %}                                                                 {% if form.tva is defined %}                                    <div class="col-sm-4">                                        <div class="form-group">                                            {{ form_label(form.tva) }}                                            {#<div class="input-group">#}                                                            {#<span class="input-group-addon">#}                                                            {#<i class="fa fa-key"></i>#}                                                            {#</span>#}                                                {{ form_widget(form.tva) }}                                            {#</div>#}                                        </div>                                    </div>                                {% endif %}                                                                 {% if form.lastNameRepresentative is defined %}                                    <div class="col-sm-6">                                        <div class="form-group">                                            {{ form_label(form.lastNameRepresentative) }}                                            <div class="input-group">                                                            <span class="input-group-addon">                                                            <i class="fa fa-user"></i>                                                            </span>                                                {{ form_widget(form.lastNameRepresentative) }}                                            </div>                                        </div>                                    </div>                                {% endif %}                                  {% if form.email is defined %}                                    <div class="col-sm-6">                                        <div class="form-group">                                            {{ form_label(form.email) }}                                            <div class="input-group">                                                            <span class="input-group-addon">                                                            <i class="fa fa-envelope"></i>                                                            </span>                                                {{ form_widget(form.email) }}                                            </div>                                        </div>                                    </div>                                {% endif %}                                                                {% if form.firstNameRepresentative is defined %}                                    <div class="col-sm-6">                                        <div class="form-group">                                            {{ form_label(form.firstNameRepresentative) }}                                            <div class="input-group">                                                            <span class="input-group-addon">                                                            <i class="fa fa-user"></i>                                                            </span>                                                {{ form_widget(form.firstNameRepresentative) }}                                            </div>                                        </div>                                    </div>                                {% endif %}                                                                                                  <div class="col-sm-12">                                    <h4 class="title-header">Adresse Société</h4>                                </div>                                {% if form.address is defined %}                                    <div class="col-sm-6">                                        <div class="form-group">                                            {{ form_label(form.address) }}                                            <div class="input-group">                                                            <span class="input-group-addon">                                                            <i class="fa fa-phone"></i>                                                            </span>                                                {{ form_widget(form.address) }}                                            </div>                                        </div>                                    </div>                                {% endif %}                                {% if form.additionalAddress is defined %}                                    <div class="col-sm-6">                                        <div class="form-group">                                            {{ form_label(form.additionalAddress) }}                                            <div class="input-group">                                                            <span class="input-group-addon">                                                            <i class="fa fa-phone"></i>                                                            </span>                                                {{ form_widget(form.additionalAddress) }}                                            </div>                                        </div>                                    </div>                                {% endif %}                                {% if form.country is defined %}                                    <div class="col-sm-5">                                        <div class="form-group">                                            {{ form_label(form.country) }}                                            <div class="input-group">                                                            <span class="input-group-addon">                                                            <i class="fa fa-phone"></i>                                                            </span>                                                {{ form_widget(form.country) }}                                            </div>                                        </div>                                    </div>                                {% endif %}                                                               {% if form.city is defined %}                                    <div class="col-sm-5">                                        <div class="form-group">                                            {{ form_label(form.city) }}                                            <div class="input-group">                                                            <span class="input-group-addon">                                                            <i class="fa fa-phone"></i>                                                            </span>                                                {{ form_widget(form.city) }}                                            </div>                                        </div>                                    </div>                                {% endif %}                                                                 {% if form.postalCode is defined %}                                    <div class="col-sm-2">                                        <div class="form-group">                                            {{ form_label(form.postalCode) }}                                            <div class="input-group">                                                            <span class="input-group-addon">                                                            <i class="fa fa-phone"></i>                                                            </span>                                                {{ form_widget(form.postalCode) }}                                            </div>                                        </div>                                    </div>                                {% endif %}                                {% if form.phone is defined %}                                    <div class="col-sm-6">                                        <div class="form-group">                                            {{ form_label(form.phone) }}                                            <div class="input-group">                                                            <span class="input-group-addon">                                                            <i class="fa fa-phone"></i>                                                            </span>                                                {{ form_widget(form.phone) }}                                            </div>                                        </div>                                    </div>                                {% endif %}                                {% if form.mobile is defined %}                                    <div class="col-sm-6">                                        <div class="form-group">                                            {{ form_label(form.mobile) }}                                            <div class="input-group">                                                            <span class="input-group-addon">                                                            <i class="fa fa-phone"></i>                                                            </span>                                                {{ form_widget(form.mobile) }}                                            </div>                                        </div>                                    </div>                                {% endif %}                            </div>                        </div>                        <div class="col-lg-4 col-md-4 text-center">                            {#<div class="row">#}                                {% if form.image is defined %}                                    {#<div class="col-lg-12 col-md-12 ">#}                                        <div class="form-group" style="display: none;">                                            {{ form_label(form.image) }}                                        </div>                                        <div class="fileinput fileinput-new " data-provides="fileinput">                                            <div class=" fileinput-new thumbnail">                                                <img id="img_preview_background"                                                     src="{{ asset('assets/img/no_image_landscape.png') }}"                                                     alt="" class="image-responsive"/></div>                                            <div class="fileinput-preview fileinput-exists thumbnail"></div>                                            <div>                                        <span class="btn default btn-file">                                            <span class="fileinput-new"> Selectionner une image </span>                                            <span class="fileinput-exists"> Changer l'image</span>                                            {{ form_widget(form.image) }}</span>                                                <a href="javascript:;" class="btn red fileinput-exists"                                                   data-dismiss="fileinput"> Supprimer </a>                                            </div>                                        </div>                                    {#</div>#}                                {% endif %}                            {#</div>#}                        </div>                    </div>                    {{ form_end(form) }}                </div>            </div>        </div>    </div>{% endblock back_body %}{% block javascripts %}    {{ parent() }}    {% javascripts    'bundles/back/theme/assets/global/plugins/select2/js/select2.full.min.js'    "bundles/back/theme/assets/global/plugins/bootstrap-fileinput/bootstrap-fileinput.js"    "bundles/back/js/company/add.js" %}    <script src="{{ asset_url }}"></script>    {% endjavascripts %}{% endblock %}